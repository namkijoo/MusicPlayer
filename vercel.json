{
  "name": "nkj-player",
  "builds": [
    {
      "src": "dist/**", // 빌드 결과물이 dist에 있는 경우
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
